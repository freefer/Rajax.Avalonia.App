<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Design.PreviewWith>
		<Border Padding="20">
			<StackPanel>
				<Button Padding="10,6">
					<DockPanel>
						<Image Source="/Assets/play.png" Margin="0,0,7,0"></Image>
						<TextBlock >确定开始</TextBlock>
					</DockPanel>
				</Button>
			</StackPanel>
		</Border>
	</Design.PreviewWith>
	<Style>
		<Style.Resources>

		</Style.Resources>
	</Style>
	<Style Selector="TextBlock.b1">
		<Setter Property="FontSize" Value="24"/>
		<Setter Property="FontWeight" Value="Bold"/>
		<Setter Property="Foreground" Value="#FFFF" />

	</Style>

	<Style Selector="Button">
		<Setter Property="Cursor" Value="Hand"></Setter>
		<Setter Property="Padding" Value="20,6"></Setter>
		<Setter Property="BorderThickness" Value="1"></Setter>
		<Setter Property="BorderBrush" Value="#e1e1e2" />
		<Setter Property="Background" Value="#ffffff" />
		<Setter Property="Height" Value="26"></Setter>
		<Setter Property="Foreground" Value="Black"></Setter>
		<Setter Property="FontSize" Value="12"></Setter>
		<!--<Setter Property="FontFamily" Value="宋体"></Setter>-->
	</Style>

